{
  "version": 2,
  "builds": [
    { "src": "Site/(.*)", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/", "dest": "Site/index.html" },
    { "src": "/questionarios", "dest": "Site/questionarios.html" },
    { "src": "/diagnostico", "dest": "Site/diagnostico.html" },
    { "src": "/(.*)", "dest": "Site/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "public, max-age=300" }
      ]
    }
  ]
}
